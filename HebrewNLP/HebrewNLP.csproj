<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.6</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://hebrew-nlp.co.il/assets/eula.html</PackageLicenseUrl>
    <Copyright>Copyright 2018 hebrew-nlp.co.il</Copyright>
    <Description>C# Library for HebrewNLP API</Description>
    <RepositoryUrl>https://github.com/HebrewNLP/HebrewNLP.NET</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageProjectUrl>https://hebrew-nlp.co.il</PackageProjectUrl>
    <PackageId>HebrewNLP.NET</PackageId>
    <PackageTags>nlp natrual-language-processing hebrew</PackageTags>
    <PackageReleaseNotes>+ Added NameAnalyzer for analyzing names

* The NameTranslation URL has changed, meaning existing code using the NameTranslation must be updated</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

</Project>
